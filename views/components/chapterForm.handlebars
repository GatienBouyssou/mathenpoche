<div class="row">
    <form action="admin/3rdLevel.html" method="post" class="col s12">
        <div class="row">
            <div class="col s12 m2 valign-wrapper customValign">
                <span>Chapter : </span>
            </div>
            <div class="input-field col s12 m6">
                <input placeholder="Title" id="chapterTitle" type="text" name="title" class="validate" value="{{element.title}}" required>
                <label for="chapterTitle">Title</label>
            </div>
        </div>
        <div class="row">
            <button class="btn waves-effect waves-light" type="submit" name="action" data-type="{{elementType}}" data-level="{{levelName}}" data-id="{{element._id}}">Submit
                <i class="material-icons right">send</i>
            </button>
        </div>
    </form>
</div>